<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fetcher.controller.MainController">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox>
               <children>
                  <HBox alignment="TOP_CENTER" maxHeight="200.0">
                     <children>
                        <ImageView pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@plus.png" />
                           </image>
                        </ImageView>
                         <ImageView pickOnBounds="true" preserveRatio="true">
                             <image>
                                 <Image url="@delete.png" />
                             </image>
                         </ImageView>
                         <ImageView pickOnBounds="true" preserveRatio="true">
                             <image>
                                 <Image url="@sortdate.png" />
                             </image>
                         </ImageView>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                   <ListView fx:id="listURL" style="-fx-padding: 10px;" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
